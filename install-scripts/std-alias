#
# startand alias definitions
#

alias hi='history'
alias rm='/bin/rm -i'
alias lc='/bin/ls -c'
alias l='/bin/ls -la'
alias lh='/bin/ls -lt | head -50'

alias ut='exit'

#################################################################################
# git aliases
alias gst='git status'
alias gbr='git branch -a | head -20'
alias gco='git checkout'
alias gcom='git commit'
alias gac='git add . ; git commit -v'

